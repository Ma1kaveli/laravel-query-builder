{
    "name": "makaveli/laravel-query-builder",
    "description": "Query Builder for Laravel",
    "keywords": ["laravel", "framework"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Udovenko",
            "email": "mikhail.udovenko.02@mail.ru"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.10|^11.0|^12.0",
        "makaveli/laravel-core": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "QueryBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "QueryBuilder\\Providers\\QueryBuilderServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
